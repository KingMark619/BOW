<view class="fui-cell-group fui-cell-click" style="margin-top:0" wx:if="{{member.needbind==1}}">
    <navigator class="fui-cell" hoverClass="none" openType="navigate" url="/pages/member/info/index">
        <view class="fui-cell-icon">
            <text class="icox icox-mobilephone"></text>
        </view>
        <view class="fui-cell-text" bindtap='onTapDayWeather'>
            <p class="text text-danger">Be tied to a phone number</p>
        </view>
        <view class="fui-cell-remark"></view>
    </navigator>
    <view class="fui-cell-tip">{{member.bindtext}}</view>
</view>
