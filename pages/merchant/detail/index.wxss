.title {
  font-size: 20px;
  letter-spacing: 0.2px;
  font-weight: 500;
  color: black;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  display: flex;
  white-space: nowrap;
  overflow-x: hidden;
  width: 100%;
}
.title:before, .title:after {
  border-top: 2px solid #ff4c4c;
  content: '';
  display: table-cell;
  position: relative;
  top: 1.8em;
  width: 45%;
}

.title:before {
  right: 3%;
}

.title:after {
  left: 3%;
}
