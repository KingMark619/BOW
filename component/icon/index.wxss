/* component/icon/index.wxss */
.icon-block {
  width: 100%;
  height: auto;
  margin: auto;
  margin-top: 10px;
  margin-bottom: 10px;
}
.row {
  width: 100%;
  height: 100px;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.icon {
  display: flex;
  flex-direction: column;
  height: 64px;
  width: 25%;
}
.one {
  order: 1;
  margin-left: -4%;
}
.two {
  order: 2
}
.three {
  order: 3;
}
.four {
  order: 4;
}
.five {
  order: 5;
}
.six {
  order: 6;
}
.filter {
  filter: contrast(10%)
}
.icon-image {
  width: 32px;
  height: 32px;
  margin: auto;
}
.icon-text {
  font-size: 9px;
  color: black;
  text-align: center;
  letter-spacing: -0.2px;
}